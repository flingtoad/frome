# Initial SSH settings for ansible connect.
ansible_ssh_port: 22 
ansible_ssh_user: user
ansible_ssh_private_key_file: ~/.ssh/id_rsa

# CSF settings.
csf_config: csf.conf

# Server-specific names and domains
server_hostname: host01
server_domain: example.com

# Password (hashed) for newly created user
user_create: user
user_pass: hashed_password 


